          <br/>\n                <div className=\"filterPrice\">\n                    <h2>Prices:</h2>\n                            <label>\n                                <input type=\"radio\" name=\"priceRange\" value=\"1_to_50\" onChange={filterRangePrices}/> $1 - $50\n                            </label><br/>\n\n                            <label>\n                                <input type=\"radio\" name=\"priceRange\" value=\"51_to_100\" onChange={filterRangePrices} /> $51 - $100\n                            </label><br/>\n\n                            <label>\n                                <input type=\"radio\" name=\"priceRange\" value=\"101_to_200\" onChange={filterRangePrices}/> $101 - $200\n                            </label>\n                </div>\n                <br/>\n                <div className=\"sortProducts\">\n                    <h2>Sort by:</h2>\n                        <select name=\"price\" onChange={sortHigherPriceRating}>\n                            <option value=\"default\"></option>\n                            <option value=\"Prices\">Price</option>\n                            <option value=\"Rating\">Rating</option>\n                        </select>\n                </div>\n            </div>\n            <div className = \"v1\"></div>\n        </div>\n    )\n}\n\n/*const mapStateToProps = (state) => { //rootreducer\n    return {\n        basics: state.fetchR.basics,\n        rating: state.fetchR.rating,\n        pricesRange: state.fetchR.pricesRange,\n        filteredProducts: state.fetchR.filteredProducts,\n        filterPrices: state.fetchR.filterPrices,\n    }\n}*/\n\nconst mapDispatchToProps = dispatch => {\n    return {\n        basicsClick: (filterOption) => dispatch(filterBasics(filterOption)),\n        basicsClickRemove: () => dispatch(filterBasicsRemove()),\n        sortOptionClick: (sortOption) => dispatch(sortRatingPrices(sortOption)),\n        sortOptionClickRemove: () => dispatch(sortRemove()),\n        filterPrices: (priceFilter) => dispatch(filterPrices(priceFilter)),\n    }\n}\n\nexport default connect(null, mapDispatchToProps)(Filters);","C:\\Users\\Lissette.vasquez\\Documents\\React\\Proyecto Curso React - Globant\\ReactJS_cart_shop-master\\src\\Store\\actions\\actions.js",[],"C:\\Users\\Lissette.vasquez\\Documents\\React\\Proyecto Curso React - Globant\\ReactJS_cart_shop-master\\src\\Components\\product\\Product.js",[],"C:\\Users\\Lissette.vasquez\\Documents\\React\\Proyecto Curso React - Globant\\ReactJS_cart_shop-master\\src\\Store\\actions\\index.js",[],"C:\\Users\\Lissette.vasquez\\Documents\\React\\Proyecto Curso React - Globant\\ReactJS_cart_shop-master\\src\\myHooks\\myHooks.js",["224"],"import React, { useState, useRef} from \"react\";\n\nexport default function useScrollState(initialValue) {\n    \n    const [scrollState, setScrollState] = useState(initialValue);\n    const scrollRef = useRef(scrollState);\n\n    const setCurrentScrollState = value => {\n        scrollRef.current = value;\n        setScrollState(value);\n    };\n\n    return [scrollRef, setCurrentScrollState];\n}","C:\\Users\\Lissette.vasquez\\Documents\\React\\Proyecto Curso React - Globant\\ReactJS_cart_shop-master\\src\\Store\\actions\\cartActions.js",[],"C:\\Users\\Lissette.vasquez\\Documents\\React\\Proyecto Curso React - Globant\\ReactJS_cart_shop-master\\src\\Containers\\header\\Header.js",["225","226"],"import React, { useState } from 'react';\nimport './Header.css';\nimport ShoppingCartLogo from '../../Components/modalCart/logoCart';\nimport globantShops from '../../assets/globant-shops.svg';\n\nconst Header = (props) => {\n\n    //Abrir y cerrar carrito\n    const [show, setShow] = useState(false)\n    const closeModalHandler = () => setShow(false);\n\n    return (\n        <header>\n            <img src = {globantShops} alt=\"logo\" />\n            <ShoppingCartLogo clicked={props.clicked} productCounter={props.productCounter} />\n        </header>\n    );\n}\n\nexport default Header;","C:\\Users\\Lissette.vasquez\\Documents\\React\\Proyecto Curso React - Globant\\ReactJS_cart_shop-master\\src\\Components\\comments\\comments.js",[],"C:\\Users\\Lissette.vasquez\\Documents\\React\\Proyecto Curso React - Globant\\ReactJS_cart_shop-master\\src\\Components\\seeMoreBtn\\seeMoreBtn.js",[],"C:\\Users\\Lissette.vasquez\\Documents\\React\\Proyecto Curso React - Globant\\ReactJS_cart_shop-master\\src\\Components\\modalCart\\logoCart.js",[],"C:\\Users\\Lissette.vasquez\\Documents\\React\\Proyecto Curso React - Globant\\ReactJS_cart_shop-master\\src\\Components\\product\\productCartCard.js",[],"C:\\Users\\Lissette.vasquez\\Documents\\React\\Proyecto Curso React - Globant\\ReactJS_cart_shop-master\\src\\Containers\\success\\Success.js",[],"C:\\Users\\Lissette.vasquez\\Documents\\GitHub\\ReactJS_cart_shop\\src\\index.js",[],["227","228"],"C:\\Users\\Lissette.vasquez\\Documents\\GitHub\\ReactJS_cart_shop\\src\\reportWebVitals.js",[],"C:\\Users\\Lissette.vasquez\\Documents\\GitHub\\ReactJS_cart_shop\\src\\App.js",[],"C:\\Users\\Lissette.vasquez\\Documents\\GitHub\\ReactJS_cart_shop\\src\\Store\\reducers\\index.js",[],"C:\\Users\\Lissette.vasquez\\Documents\\GitHub\\ReactJS_cart_shop\\src\\Store\\reducers\\fetchReducer.js",[],"C:\\Users\\Lissette.vasquez\\Documents\\GitHub\\ReactJS_cart_shop\\src\\Containers\\header\\Header.js",["229"],"import React, { useState } from 'react';\nimport './Header.css';\nimport ShoppingCartLogo from '../../Components/modalCart/logoCart';\nimport globantShops from '../../assets/globant-shops.svg';\nimport { useLocation } from 'react-router-dom';\nimport { connect } from 'react-redux';\n\nconst Header = (props) => {\n\n    const { clicked, productCounter } = props;\n    const location = useLocation();\n\n    return (\n        <header>\n            <img src = {globantShops} alt=\"logo\" />\n            {location.pathname === '/ordering' || location.pathname === '/success' ?\n            null\n            : <ShoppingCartLogo clicked={clicked} productCounter={productCounter} />}\n        </header>\n    );\n}\n\nconst mapStateToProps = (state) => {\n    return {\n        productCounter: state.cartR.productCounter,\n    }\n}\n\nexport default connect(mapStateToProps)(Header);","C:\\Users\\Lissette.vasquez\\Documents\\GitHub\\ReactJS_cart_shop\\src\\Store\\reducers\\cartReducer.js",[],"C:\\Users\\Lissette.vasquez\\Documents\\GitHub\\ReactJS_cart_shop\\src\\Containers\\form\\Form.js",["230"],"C:\\Users\\Lissette.vasquez\\Documents\\GitHub\\ReactJS_cart_shop\\src\\Containers\\details\\Details.js",[],"C:\\Users\\Lissette.vasquez\\Documents\\GitHub\\ReactJS_cart_shop\\src\\Containers\\displayProducts\\DisplayProducts.js",["231","232"],"import React, {useEffect} from 'react';\nimport './DisplayProducts.css';\nimport Filters from '../../Components/filters/Filters';\nimport Product from '../../Components/product/Product';\nimport { connect } from 'react-redux';\nimport * as actions from '../../Store/actions/index';\nimport useScrollState from '../../myHooks/myHooks';\n\n//HOOK VERSION\nconst DisplayProducts = (props) => {\n\n    const { fetchProducts, addPage, products, response, page, filterOption, sortOption, pricesRange, filteredProducts, filterPricesOn } = props;\n    const [responseRef, setResponseRef] = useScrollState(response);\n    \n    //Data products\n    useEffect (() => {\n        fetchProducts(page, filterOption, sortOption, filterPricesOn, pricesRange);\n    }, [fetchProducts, page, filterOption, sortOption])\n\n    //Filter\n    let productsToRender = [];\n\n    if (filterPricesOn) {\n        productsToRender = filteredProducts;\n    } else {\n        productsToRender = products;\n    }\n    \n    //Scroll\n    useEffect (() => {\n        setResponseRef(response)\n    }, [response, setResponseRef]);\n\n    function scrolling (event) {\n        \n        const { scrollTop, clientHeight, scrollHeight } = event.srcElement.documentElement;\n\n        console.log('entro al evento')\n        \n        if (Math.ceil(scrollHeight - scrollTop) === clientHeight) {\n            console.log('entro al primer if')\n            if (!(responseRef.current.currentPage === responseRef.current.pages)) {\n                console.log('entro a cambiar pagina')\n                addPage();\n            }\n        };\n    }\n\n    useEffect(() => {\n        if (window) {\n            window.addEventListener('scroll', scrolling);\n        }\n        return () => {\n            window.removeEventListener('scroll', scrolling);\n        }\n    }, [])\n\n    return (\n        <div className =\"productsBox\">\n            <Filters\n            />\n            <div>\n                <h1 className=\"productsTitle\">Our products:</h1>\n                <div className=\"productsGrid\">\n                    {productsToRender.map(( product => {\n                        return(\n                        <Product \n                        key={product.id}   \n                        productId={product.id}\n                        productImg={product.img}\n                        productName={product.name}\n                        productCom={product.comments}\n                        productPrice={product.price}\n                        productBasics={product.basics}\n                        productRate={product.rate}\n                        productAdded={()=> props.addProductCart(product)}\n                        />\n                        )\n                    })\n                    )}\n                </div>\n            </div>\n        </div>\n    )\n}\n\nconst mapStateToProps = (state) => { //rootreducer acceder a las variables que componen mi estado global, visualizar, validación\n    return {\n        products: state.fetchR.products,\n        response: state.fetchR.response,\n        page: state.fetchR.page,\n        filterOption: state.fetchR.filterOption,\n        sortOption: state.fetchR.sortOption,\n        pricesRange: state.fetchR.pricesRange,\n        filteredProducts: state.fetchR.filteredProducts,\n        filterPricesOn: state.fetchR.filterPricesOn,\n    }\n}\n\nconst mapDispatchToProps = (dispatch) => { //traer action creator \n    return{\n        fetchProducts: (page, filterOption, sortOption, pricesRange, filteredProducts, filterPricesOn) => dispatch(actions.fetchProducts(page, filterOption, sortOption, pricesRange, filteredProducts, filterPricesOn)),\n        addPage: () => dispatch(actions.addPage()),\n        addProductCart: (productInfo) => dispatch(actions.addProductCart(productInfo))\n    }\n}\n\nexport default connect(mapStateToProps, mapDispatchToProps)(DisplayProducts);\n\n//CLASS VERSION\n\n//import React,{ Component } from 'react';\n\n// import Product from '../../Components/product/Product';\n\n// class DisplayProducts extends Component {\n//     constructor(props) {\n//         super(props);\n//         this.state = { products: [], filteredProducts: [] };\n//     }\n    \n//     componentWillMount() {\n//         fetch(`http://localhost:8080/products`).then(res => res.json())\n//         .then(data => this.setState({\n//             products: data.products,\n//             filteredProducts: data.products,\n//         }));\n//     }\n\n//     render () {\n//         return (\n//         <div>\n//             <h1 className=\"productsTitle\"> Our products: </h1>\n//                 <div className = \"productsGrid\">\n//                 <Product products={this.state.filteredProducts} /*handleAddToCart={this.handleAddToCart}*/ />\n//                 </div>\n//         </div>\n//         )\n//     }\n// }\n\n// export default DisplayProducts;\n\n\n\n\n\n\n\n\n\n\n","C:\\Users\\Lissette.vasquez\\Documents\\GitHub\\ReactJS_cart_shop\\src\\Containers\\success\\Success.js",[],"C:\\Users\\Lissette.vasquez\\Documents\\GitHub\\ReactJS_cart_shop\\src\\Components\\modalCart\\ModalCart.js",[],"C:\\Users\\Lissette.vasquez\\Documents\\GitHub\\ReactJS_cart_shop\\src\\Components\\modalCart\\logoCart.js",[],"C:\\Users\\Lissette.vasqu